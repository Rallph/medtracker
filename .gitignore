# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore dotfiles in coverage directory
coverage/.*
/.idea/.gitignore
/.idea/misc.xml
/.idea/modules.xml
/.idea/selt_2021_proj_09.iml
/.idea/vcs.xml
/.externalDependencies.xml
